                        <TableColumn fx:id="precioTable" prefWidth="190.39996337890625" text="Precio" />
                  <TableColumn fx:id="feriaTable" prefWidth="190.39996337890625" text="Feria" />
                  <TableColumn fx:id="asistenteTable" prefWidth="190.39996337890625" text="Asistente" />
                    </columns>
                </TableView>
            <Button fx:id="facturaButton" layoutX="415.0" layoutY="421.0" mnemonicParsing="false" onAction="#generarFactura" text="Generar Factura" />
                <Button fx:id="buttonInicio" mnemonicParsing="false" onAction="#buttonInicio" text="Volver Al Inicio" />
            </children>
        </VBox>
    </children>
</AnchorPane>
